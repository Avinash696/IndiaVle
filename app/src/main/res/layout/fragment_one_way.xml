<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                app:cardBackgroundColor="@color/white"
                style="@style/CustomCardCorners"
                app:cardPreventCornerOverlap="true"
                app:cardUseCompatPadding="true">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/top_card">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="60dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:padding="6dp">

                            <TextView
                                android:id="@+id/tvFrom"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_marginStart="20dp"
                                android:text="From"
                                android:textSize="12sp"
                                android:textStyle="bold" />

                            <EditText
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/tvFrom"
                                android:layout_centerHorizontal="true"
                                android:hint="Sydney"
                                android:textColor="@color/sf_green"
                                android:textColorHint="@color/sf_green" />
                        </RelativeLayout>

                        <View
                            android:layout_width="40dp"
                            android:layout_height="50dp"
                            android:layout_marginTop="20dp"
                            android:layout_weight="1"
                            android:background="@drawable/flight_icon" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="10dp"
                            android:layout_weight="1"
                            android:padding="6dp">

                            <TextView
                                android:id="@+id/tvTo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_marginStart="20dp"
                                android:text="To"
                                android:textSize="12sp"
                                android:textStyle="bold" />

                            <EditText
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/tvTo"
                                android:layout_centerHorizontal="true"
                                android:hint="India"
                                android:textColor="@color/sf_green"
                                android:textColorHint="@color/sf_green" />
                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="60dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:padding="6dp">

                            <TextView
                                android:id="@+id/tvDepart"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_marginStart="20dp"
                                android:text="Depart"
                                android:textSize="12sp"
                                android:textStyle="bold" />

                            <androidx.constraintlayout.utils.widget.ImageFilterView
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_below="@+id/tvDepart"
                                android:layout_toLeftOf="@+id/etDepartDate"
                                android:src="@drawable/date_picker" />

                            <EditText
                                android:id="@+id/etDepartDate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/tvDepart"
                                android:layout_centerHorizontal="true"
                                android:hint="Select Date"
                                android:textColor="@color/sf_green"
                                android:textColorHint="@color/sf_green" />
                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="10dp"
                            android:layout_weight="1"
                            android:padding="6dp">

                            <TextView
                                android:id="@+id/tvReturn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_marginStart="20dp"
                                android:text="Retun"
                                android:textSize="12sp"
                                android:textStyle="bold" />

                            <androidx.constraintlayout.utils.widget.ImageFilterView
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_below="@+id/tvReturn"
                                android:layout_toLeftOf="@+id/etRetunDate"
                                android:src="@drawable/date_picker" />

                            <EditText
                                android:id="@+id/etRetunDate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/tvReturn"
                                android:layout_centerHorizontal="true"
                                android:hint="Select Date"
                                android:textColor="@color/sf_green"
                                android:textColorHint="@color/sf_green" />
                        </RelativeLayout>

                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="20dp"
                        android:text="@string/passangers_luggage"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:orientation="horizontal"
                        android:layout_marginLeft="4dp">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="4dp"
                            android:src="@drawable/audience" />

                        <Spinner
                            android:id="@+id/spPassangers"
                            android:layout_width="30dp"
                            android:layout_height="30dp" />

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="40dp"
                            android:src="@drawable/little" />

                        <Spinner
                            android:id="@+id/spKids"
                            android:layout_width="30dp"
                            android:layout_height="30dp" />

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="40dp"
                            android:src="@drawable/weight" />

                        <Spinner
                            android:id="@+id/spWeight"
                            android:layout_width="30dp"
                            android:layout_height="30dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:orientation="horizontal"
                        android:layout_gravity="center">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="4dp"
                            android:src="@drawable/economy" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_centerVertical="true"
                                android:text="Economy" />
                        </RelativeLayout>


                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="40dp"
                            android:src="@drawable/bussiness_class" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:text="Business " />
                        </RelativeLayout>

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="40dp"
                            android:src="@drawable/elite" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:text="Elite" />
                        </RelativeLayout>
                    </LinearLayout>

                        <androidx.cardview.widget.CardView
                            android:layout_width="190dp"
                            android:layout_height="30dp"
                            app:cardCornerRadius="40dp"
                            android:layout_gravity="center"
                            android:layout_marginTop="40dp"
                            app:cardBackgroundColor="#FF715D"
                            android:layout_marginBottom="10dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Search Flight"
                                android:layout_gravity="center" />
                        </androidx.cardview.widget.CardView>

                </LinearLayout>

            </RelativeLayout>
            </com.google.android.material.card.MaterialCardView>

        </ScrollView>
